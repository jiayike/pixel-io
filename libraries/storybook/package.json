{
  "name": "storybook",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-storybook": "test-storybook",
    "test": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && pnpx http-server storybook-static --port 6007 --silent\" \"wait-on tcp:6007 && pnpm test-storybook --url http://127.0.0.1:6007\"",
    "test:watch": "pnpm test-storybook --watch",
    "dev": "pnpm storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf ./storybook-static",
    "postinstall": "pnpx playwright install && pnpx playwright install-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@pixel-io/components": "workspace:*",
    "@pixel-io/styles": "workspace:*",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/web-components": "^8.0.9",
    "@storybook/web-components-vite": "^8.0.9",
    "@types/jest-image-snapshot": "^6.4.0",
    "concurrently": "^8.2.2",
    "jest-image-snapshot": "^6.4.0",
    "lit-html": "^3.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.0.9",
    "vite": "^5.2.10",
    "wait-on": "^7.2.0"
  }
}
