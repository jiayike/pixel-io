{
  "name": "pixel-io",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --stream --parallel dev",
    "build": "pnpm --stream --parallel build",
    "build:docs": "pnpm build && pnpm -F storybook build-storybook",
    "test": "pnpm --stream --parallel test",
    "test:watch": "pnpm --stream --parallel test:watch",
    "prepare": "husky install",
    "commit": "cz",
    "commit:retry": "cz --retry",
    "reset": "pnpm -r exec rm -rf node_modules && rm -rf node_modules && rm -f pnpm-lock.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiayike/pixel-io.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jiayike/pixel-io/issues"
  },
  "homepage": "https://github.com/jiayike/pixel-io#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3"
  }
}
