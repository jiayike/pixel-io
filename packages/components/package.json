{
  "name": "@pixel-ui/components",
  "private": true,
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/components.umd.js",
  "module": "./dist/components.mjs",
  "exports": {
    ".": {
      "import": "./dist/components.mjs",
      "require": "./dist/components.umd.js"
    }
  },
  "scripts": {
    "dev": "pnpm build -- --watch",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "lint": "eslint ./src/**/*.{js,svelte,ts}",
    "lint:fix": "pnpm lint -- --fix",
    "prelint": "pnpm format",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "precheck": "pnpm lint:fix"
  },
  "devDependencies": {
    "@pixel-ui/styles": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^1.0.8",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "sass": "^1.55.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  }
}
