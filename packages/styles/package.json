{
  "name": "@pixel-ui/styles",
  "private": true,
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/styles.umd.js",
  "module": "./dist/styles.mjs",
  "exports": {
    ".": {
      "import": "./dist/styles.mjs",
      "require": "./dist/styles.umd.js"
    }
  },
  "type": "commonjs",
  "scripts": {
    "dev": "pnpm build -- --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write ./src/**/*.{js,html,ts}",
    "lint": "eslint ./src/**/*.{js,ts}",
    "lint:fix": "pnpm lint -- --fix",
    "prelint": "pnpm format",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint:fix": "pnpm stylelint -- --fix",
    "check": "pnpm stylelint:fix && pnpm lint:fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "stylelint": "^14.12.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-stylelint": "^3.0.7"
  }
}
